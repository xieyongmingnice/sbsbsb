<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.xunyu.provider.pojo.User">

    <resultMap id="user_ResultMap" type="com.xunyu.provider.pojo.User">
        <id column="id" property="userId"></id>
        <result column="name" property="userName"></result>
    </resultMap>
    <select id="listUser" parameterType="HashMap" resultMap="user_ResultMap">
        select u.id ,u.name from  user u where 1=1 order by u.id desc
            limit #{startRow},#{endRow}
    </select>

</mapper>